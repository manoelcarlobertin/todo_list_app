<h1><i class="fas fa-tasks"></i> Lista de Tarefas</h1>

<div id="task_lists" class="row">
  <% @task_lists.each do |task_list| %>
    <div class="col-md-4 mb-4">
      <%= render 'list_card', task_list: task_list %>
    </div>
  <% end %>
</div>

<div class="d-flex justify-content-center">
  <%= paginate @task_lists %>
</div>
